{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: \"SignUp\",\n  data() {\n    return {\n      showErrorEmail: false,\n      formData: {\n        email: \"\",\n        password: \"\"\n      }\n    };\n  },\n  methods: {\n    register() {\n      var bodyFormData = new FormData();\n      bodyFormData.append('email', this.email);\n      bodyFormData.append('password', this.password);\n      // Validar el correo electrónico\n      if (!this.email.endsWith(\"@ipn.mx\")) {\n        // Mostrar mensaje de error o tomar la acción correspondiente\n        this.showErrorEmail = true;\n        console.error(\"El correo electrónico debe tener la terminación '@ipn.mx'\");\n        return;\n      }\n      // Realizar la solicitud HTTP utilizando Axios\n      axios.post(\"http://localhost/Registro.php\", this.formData).then(response => {\n        // Redireccionar a otro componente después de realizar el registro\n        console.log(response);\n        console.log(response.data);\n        this.$router.push(\"/login-page\");\n        console.log(this.formData.email);\n        console.log(this.formData.password);\n      }).catch(error => {\n        this.showErrorEmail = false;\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AA4CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAI,GAAG;IACL,OAAO;MACLC,cAAc,EAAE,KAAK;MACrBC,QAAQ,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQ,GAAG;MACT,IAAIC,YAAW,GAAI,IAAIC,QAAQ,EAAE;MACjCD,YAAY,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACN,KAAK,CAAC;MACxCI,YAAY,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACL,QAAQ,CAAC;MAC9C;MACA,IAAI,CAAC,IAAI,CAACD,KAAK,CAACO,QAAQ,CAAC,SAAS,CAAC,EAAE;QACnC;QACA,IAAI,CAACT,cAAa,GAAI,IAAI;QAC1BU,OAAO,CAACC,KAAK,CAAC,2DAA2D,CAAC;QAC1E;MACF;MACA;MACAd,KAAI,CACDe,IAAI,CAAC,+BAA+B,EAAE,IAAI,CAACX,QAAQ,EACnDY,IAAI,CAACC,QAAO,IAAK;QAChB;QACAJ,OAAO,CAACK,GAAG,CAACD,QAAQ,CAAC;QAC