{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'LobbyTwo',\n  data() {\n    return {\n      nombre: '',\n      email: '',\n      password: '',\n      apellido: '',\n      boleta: '',\n      telefono: '',\n      escuela: '',\n      planRelacion: '',\n      descripcion: ''\n    };\n  },\n  created() {\n    // Obtener los datos del Local Storage\n    const userData = localStorage.getItem('userData');\n    if (userData) {\n      const parsedData = JSON.parse(userData);\n      this.nombre = parsedData.nombre;\n      this.email = parsedData.email;\n      this.password = parsedData.password;\n      this.apellido = parsedData.apellido;\n      this.boleta = parsedData.boleta;\n      this.telefono = parsedData.telefono;\n      this.escuela = parsedData.escuela;\n      this.planRelacion = parsedData.planRelacion;\n      this.descripcion = parsedData.descripcion;\n\n      // Llamar al backend para obtener los datos actualizados del usuario\n      axios.get('/api/user/' + parsedData.userId).then(response => {\n        // Actualizar los datos del usuario con la respuesta del backend\n        this.nombre = response.data.nombre;\n        this.email = response.data.email;\n        this.password = response.data.password;\n        this.apellido = response.data.apellido;\n        this.boleta = response.data.boleta;\n        this.telefono = response.data.telefono;\n        this.escuela = response.data.escuela;\n        this.planRelacion = response.data.planRelacion;\n        this.descripcion = response.data.descripcion;\n        // Actualizar los demás campos según tus necesidades\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAsBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,EAAE;MACXC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE;