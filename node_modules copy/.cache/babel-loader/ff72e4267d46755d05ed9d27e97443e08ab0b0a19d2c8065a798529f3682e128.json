{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport SurveyResult from './SurveyResult.vue';\nexport default {\n  components: {\n    SurveyResult\n  },\n  data() {\n    return {\n      results: [],\n      isLoading: false\n    };\n  },\n  methods: {\n    loadExperiencies() {\n      this.isLoading = true;\n      fetch('https://vue-http-demo-c68cf-default-rtdb.firebaseio.com/surveys.json').then(response => {\n        if (response.ok) {\n          return response.json();\n        }\n      }).then(data => {\n        this.isLoading = false;\n        const results = [];\n        for (const id in data) {\n          results.push({\n            id: id,\n            name: data[id].name,\n            rating: data[id].rating\n          });\n        }\n        this.results = results;\n      });\n    }\n  },\n  mounted() {\n    this.loadExperiencies();\n  }\n};","map":{"version":3,"mappings":";AAqBA,OAAOA,YAAW,MAAO,oBAAoB;AAE7C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAI,GAAE;IACJ,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgB,GAAE;MAChB,IAAI,CAACF,SAAQ,GAAI,IAAI;MAErBG,KAAK,CAAC,sEAAsE,EAC3EC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAGA,QAAQ,CAACC,EAAE,EAAC;UACb,OAAOD,QAAQ,CAACE,IAAI,EAAE;QACxB;MACF,CAAC,CAAC,CAACH,IAAI,CAAEN,IAAI,IAAK;QAChB,IAAI,CAACE,SAAQ,GAAI,KAAK;QACtB,MAAMD,OAAM,GAAI,EAAE;QAClB,KAAI,MAAMS,EAAC,IAAKV,IAAI,EAAC;UACnBC,OAAO,CAACU,IAAI,CAAC;YACXD,EAAE,EAAEA,EAAE;YACNE,IAAI,EAAEZ,IAAI,CAACU,EAAE,CAAC,CAACE,IAAI;YACnBC,MAAM,EAAEb,IAAI,CAACU,EAAE,CAAC,CAACG;UACnB,CAAC,CAAC;QACJ;QACA,IAAI,CAACZ,OAAM,GAAIA,OAAO;MACxB,CAAC,CAAC;IACJ;EACF,CAAC;EACGa,OAAO,GAAE;IACP,IAAI,CAACV,gBAAgB,EAAE;EACzB;AACN,CAAC","names":["SurveyResult","components","data","results","isLoading","methods","loadExperiencies","fetch","then","response","ok","json","id","push","name","rating","mounted"],"sourceRoot":"","sources":["C:\\Users\\Alexis Jaimes\\Desktop\\vueProjects\\http-01-starting-setup\\src\\components\\survey\\UserExperiences.vue"],"sourcesContent":["<template>\n  <section>\n    <base-card>\n      <h2>Submitted Experiences</h2>\n      <div>\n        <base-button @click=\"loadExperiencies\">Load Submitted Experiences</base-button>\n      </div>\n      <p v-if=\"isLoading\"></p>\n      <ul v-if=\"!isLoading\">\n        <survey-result\n          v-for=\"result in results\"\n          :key=\"result.id\"\n          :name=\"result.name\"\n          :rating=\"result.rating\"\n        ></survey-result>\n      </ul>\n    </base-card>\n  </section>\n</template>\n\n<script>\nimport SurveyResult from './SurveyResult.vue';\n\nexport default {\n  components: {\n    SurveyResult,\n  },\n  data(){\n    return {\n      results: [],\n      isLoading: false\n    };\n  },\n  methods: {\n    loadExperiencies(){\n      this.isLoading = true;\n\n      fetch('https://vue-http-demo-c68cf-default-rtdb.firebaseio.com/surveys.json')\n      .then((response) => {\n        if(response.ok){\n          return response.json();\n        }\n      }).then((data) => {\n        this.isLoading = false;\n        const results = [];\n        for(const id in data){\n          results.push({\n            id: id, \n            name: data[id].name, \n            rating: data[id].rating\n          });\n        }\n        this.results = results;\n      });\n    }\n  },\n      mounted(){\n        this.loadExperiencies();\n      }\n};\n</script>\n\n<style scoped>\nul {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}