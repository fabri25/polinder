{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: \"SignUp\",\n  data() {\n    return {\n      showErrorEmail: false,\n      nombre: \"\",\n      apellido: \"\",\n      boleta: \"\",\n      escuela: \"\",\n      telefono: \"\",\n      planRelacion: \"\",\n      descripcion: \"\",\n      email: \"\",\n      password: \"\"\n    };\n  },\n  methods: {\n    register() {\n      var bodyFormData = new FormData();\n      bodyFormData.append('email', this.email);\n      bodyFormData.append('password', this.password);\n      bodyFormData.append('nombre', this.nombre);\n      bodyFormData.append('apellido', this.apellido);\n      bodyFormData.append('boleta', this.boleta);\n      bodyFormData.append('telefono', this.telefono);\n      bodyFormData.append('escuela', this.escuela);\n      bodyFormData.append('planRelacion', this.planRelacion);\n      bodyFormData.append('descripcion', this.descripcion);\n\n      // Validar el correo electrónico\n      if (!this.email.endsWith(\"@ipn.mx\")) {\n        // Mostrar mensaje de error o tomar la acción correspondiente\n        this.showErrorEmail = true;\n        console.error(\"El correo electrónico debe tener la terminación '@ipn.mx'\");\n        return;\n      }\n\n      // Realizar la solicitud HTTP utilizando Axios\n      axios({\n        method: \"post\",\n        url: \"http://localhost/registro.php\",\n        data: bodyFormData,\n        headers: {\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(response => {\n        // Guardar los datos en el Local Storage\n        const userData = {\n          email: this.email,\n          password: this.password,\n          nombre: this.nombre,\n          apellido: this.apellido,\n          boleta: this.boleta,\n          telefono: this.telefono,\n          escuela: this.escuela,\n          planRelacion: this.planRelacion,\n          descripcion: this.descripcion\n        };\n        localStorage.setItem('userData', JSON.stringify(userData));\n        console.log(response);\n        console.log(response.data);\n        // Redireccionar a \"/lobby-two\"\n        this.$router.push(\"/login-page\");\n      }).catch(error => {\n        this.showErrorEmail = false;\n        console.log(error);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAgGA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAI,GAAG;IACL,OAAO;MACLC,cAAc,EAAE,KAAK;MACrBC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQ,GAAG;MACT,IAAIC,YAAW,GAAI,IAAIC,QAAQ,EAAE;MACjCD,YAAY,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACN,KAAK,CAAC;MACxCI,YAAY,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACL,QAAQ,CAAC;MAC9CG,YAAY,CAACE,MAAM,CAAC,QAAQ,EAAE,IAAI,CAACb,MAAM,CAAC;MAC1CW,YAAY,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACZ,QAAQ,CAAC;MAC9CU,YAAY,CAACE,MAAM,CAAC,QAAQ,EAAE,IAAI,CAACX,MAAM,CAAC;MAC1CS,YAAY,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACT,QAAQ,CAAC;MAC9CO,YAAY,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAACV,OAAO,CAAC;MAC5CQ,YAAY,CAACE,MAAM,CAAC,cAAc,EAAE,IAAI,CAACR,YAAY,CAAC;MACtDM,YAAY,CAACE,MAAM,CAAC,aAAa,EAAE,IAAI,CAACP,WAAW,CAAC;;MAEpD;MACA,IAAI,CAAC,IAAI,CAACC,KAAK,CAACO,QAAQ,CAAC,SAAS,CAAC,EAAE;QACnC;QACA,IAAI,CAACf,cAAa,GAAI,IAAI;QAC1BgB,OAAO,CAACC,KAAK,CAAC,2DAA2D,CAAC;QAC1E;MACF;;MAEA;MACApB,KAAK,CAAC;QACJqB,MAAM,EAAE,MAAM;QACdC,GAAG,EAAE,+BAA+B;QACpCpB,IAAI,EAAEa,YAAY;QAClBQ,OAAO,EAAE;UAAE,cAAc,EAAE;QAAsB;MACnD,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAClB;QACA,MAAMC,QAAO,GAAI;UACff,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBR,MAAM,EAAE,IAAI,CAACA,MAAM;UACnBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,MAAM,EAAE,IAAI,CAACA,MAAM;UACnBE,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBD,OAAO,EAAE,IAAI,CAACA,OAAO;UACrBE,YAAY,EAAE,IAAI,CAACA,YAAY;UAC/BC,WAAW,EAAE,IAAI,CAACA;QACpB,CAAC;QACDiB,YAAY,CAACC,OAAO,CAAC,UAAU,EAAEC,IAAI,CAACC,SAAS,CAACJ,QAAQ,CA