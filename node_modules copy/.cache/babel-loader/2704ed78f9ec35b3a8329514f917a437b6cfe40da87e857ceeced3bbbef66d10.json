{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      email: '',\n      password: '',\n      nombre: '',\n      apellido: '',\n      boleta: '',\n      telefono: '',\n      escuela: '',\n      planRelacion: '',\n      descripcion: '',\n      showErrorDialog: false\n    };\n  },\n  methods: {\n    login() {\n      // Realizar la lógica de inicio de sesión aquí\n      // Puedes usar axios para hacer una llamada al backend\n      // con los datos de email y contraseña\n      axios.post('/api/login', {\n        email: this.email,\n        password: this.password,\n        nombre: this.nombre,\n        apellido: this.apellido,\n        boleta: this.boleta,\n        telefono: this.telefono,\n        escuela: this.escuela,\n        planRelacion: this.planRelacion,\n        descripcion: this.descripcion\n      }).then(response => {\n        // Guardar los datos del usuario en el Local Storage\n        localStorage.setItem('userData', JSON.stringify(response.data));\n\n        // Redirigir al usuario al LobbyTwo\n        this.$router.push('/lobby-two');\n      }).catch(error => {\n        console.log(error);\n        this.showErrorDialog = true;\n      });\n    },\n    closeErrorDialog() {\n      this.showErrorDialog = false;\n    }\n  }\n};","map":{"version":3,"mappings":";AAgEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,EAAE;MACXC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE,EAAE;MACfC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,KAAK,GAAG;MACN;MACA;MACA;MACAd,KAAI,CACDe,IAAI,CAAC,YAAY,EAAE;QAClBZ,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,